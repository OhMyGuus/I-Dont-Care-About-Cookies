{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["google", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "semi": [1, "always"],
    "max-len": 0,
    "no-tabs": 0,
    "require-jsdoc": 0,
    "no-multi-str": 0
  }
}
